From ad9a3cc1316dbfe854112c241447bfca439e829c Mon Sep 17 00:00:00 2001
From: Lectem <lectem@gmail.com>
Date: Wed, 5 Feb 2020 11:03:16 +0100
Subject: [PATCH] Need to prefix path vars with PACKAGE_ so that relocation
 works on all platforms

---
 cmake/aws-c-common-config.cmake | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/cmake/aws-c-common-config.cmake b/cmake/aws-c-common-config.cmake
index e867394ae..d69ddff76 100644
--- a/cmake/aws-c-common-config.cmake
+++ b/cmake/aws-c-common-config.cmake
@@ -1,6 +1,6 @@
 @PACKAGE_INIT@
 
-list(APPEND CMAKE_MODULE_PATH "@AWS_MODULE_INSTALL_DIR@")
+list(APPEND CMAKE_MODULE_PATH "@PACKAGE_AWS_MODULE_INSTALL_DIR@")
 
 set(THREADS_PREFER_PTHREAD_FLAG ON)
 find_package(Threads REQUIRED)
